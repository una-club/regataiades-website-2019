                };
                $.ajax({
                    url: gmap.url + 'callback=loadGmap',
                    dataType: 'script'
                });
            },
            load: function() {
                var draggable = null;
                var latlng = new google.maps.LatLng(47.236516, -1.551113);

                // manage drag for large / mobile devices
                if (screen_width > 767) {
                    draggable = true;
                } else {
                    draggable = false;
                }

                // set map options
                var mapOptions = {
                    zoom: 14,
                    center: latlng,
                    disableDefaultUI: true,
                    mapTypeId: google.maps.MapTypeId.ROADMAP,
                    draggable: draggable,
                    scrollwheel: false
                };
                var map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);

                // marker configuration
                var marker = new google.maps.Marker({
                    draggable: false,
                    position: latlng,
                    map: map,
                    title: app.app_name
                });

            },
            scrollTreatment: function() {
                gmap.analyseScrollPos();
                if (gmap.screenOnMap === true && gmap.active === false) {
                    gmap.active = true;
                    gmap.callScript();
                }
            }
        };
        $(function() {

            // load image depending on the screen size
            if ($('#map-canvas').length) {
                $(window).scroll(gmap.scrollTreatment);
            }


        });
    </script>
</body>

</html>
